{
    "cli": {
      "version": ">= 12.5.1",
      "appVersionSource": "remote"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      },
      "preview": {
        "distribution": "internal"
      },
      "production": {
        "autoIncrement": true
      }
    },
    "submit": {
      "production": {}
    },
    "builds": {
      "android": {
        "release": {
          "workflow": "managed",
          "distribution": "internal",
          "artifactPath": "./build/android/app-release.apk"
        }
      }
    }
  }
  